{
  "name": "tsub_dashboard_constructor",
  "version": "1.0.0",
  "main": "index.ts",
  "license": "UNLICENSED",
  "scripts": {
    "build": "NODE_ENV=production node scripts/build.js",
    "storybook": "NODE_ENV=development start-storybook -c config/storybook -p 6006 --ci",
    "storybook:build": "NODE_ENV=production build-storybook -c config/storybook -o build/storybook",
    "lint": "npm-run-all --parallel lint:*",
    "format": "prettier --write '{src,types,scripts}/**/*.{json,{j,t}s{,x},css,}'",
    "lint-fix": "npm-run-all format lint-fix:*",
    "lint:ts": "tslint '{src,types}/**/*.ts{,x}'",
    "lint-fix:ts": "tslint '{src,types}/**/*.ts{,x}' --fix",
    "lint:js": "eslint '{src,scripts,config}/**/*.js'",
    "lint-fix:js": "eslint '{src,scripts,config}/**/*.js' --fix",
    "lint:css": "stylelint 'src/**/*.css'",
    "lint-fix:css": "stylelint 'src/**/*.css' --fix",
    "generate-css-types": "node scripts/generate-css-types.js",
    "checktypes": "yarn generate-css-types && tsc --noEmit --extendedDiagnostics",
    "unit": "TZ=Europe/Moscow jest",
    "test": "npm-run-all --parallel lint checktypes unit"
  },
  "browserslist": [
    "Chrome >= 75"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn checktypes"
    }
  },
  "lint-staged": {
    "{src,types}/**/*.{ts,tsx}": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ],
    "{src}/**/*.css": [
      "prettier --write",
      "stylelint --fix",
      "git add"
    ],
    "{src,scripts,config}/**/*.{json,js}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "classnames": "2.2.6",
    "d3": "5.9.7",
    "lodash": "4.17.11",
    "pluralize-ru": "1.0.1",
    "react": "16.8.6",
    "react-dnd": "9.3.2",
    "react-dnd-html5-backend": "9.3.2",
    "react-dom": "16.8.6",
    "react-grid-layout-tmp-fork": "0.18.0",
    "react-use-dimensions": "1.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "7.4.4",
    "@babel/plugin-transform-react-constant-elements": "7.2.0",
    "@babel/plugin-transform-react-inline-elements": "7.2.0",
    "@babel/plugin-transform-runtime": "7.4.4",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-react": "7.0.0",
    "@hot-loader/react-dom": "16.8.6",
    "@nekitk/storybook-addon-smart-knobs": "5.0.0-nekitk.3",
    "@storybook/addon-actions": "5.1.9",
    "@storybook/addon-info": "5.1.11",
    "@storybook/addon-knobs": "5.1.9",
    "@storybook/react": "5.1.9",
    "@types/classnames": "2.2.9",
    "@types/d3": "5.7.2",
    "@types/enzyme": "3.10.2",
    "@types/jest": "24.0.15",
    "@types/lodash": "4.14.135",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.5",
    "@types/react-grid-layout": "0.16.6",
    "@types/react-hot-loader": "4.1.0",
    "@types/storybook__addon-actions": "3.4.3",
    "@types/storybook__addon-knobs": "5.0.3",
    "@types/storybook__react": "4.0.2",
    "babel-loader": "8.0.6",
    "babel-plugin-named-asset-import": "0.3.2",
    "chalk": "2.4.2",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "3.0.0",
    "css-modules-typescript-loader": "2.0.4",
    "cssnano": "4.1.10",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "eslint": "6.0.1",
    "file-loader": "4.0.0",
    "fork-ts-checker-webpack-plugin": "1.3.7",
    "husky": "3.0.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.8.0",
    "jest-enzyme": "7.0.2",
    "lint-staged": "9.0.1",
    "mini-css-extract-plugin": "0.7.0",
    "npm-run-all": "4.1.5",
    "ora": "3.4.0",
    "postcss-loader": "3.0.0",
    "postcss-nested": "4.1.2",
    "postcss-preset-env": "6.6.0",
    "prettier": "1.18.2",
    "react-docgen-typescript-loader": "3.2.0",
    "react-hot-loader": "4.12.0",
    "storybook-addon-react-docgen": "1.2.6",
    "stylelint": "10.1.0",
    "stylelint-config-prettier": "5.2.0",
    "stylelint-config-recommended": "2.2.0",
    "stylelint-config-standard": "18.3.0",
    "svgo": "1.2.2",
    "svgo-loader": "2.2.1",
    "terser-webpack-plugin": "1.3.0",
    "ts-jest": "24.0.2",
    "ts-loader": "6.0.4",
    "tslint": "5.18.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.0.1",
    "tslint-react": "4.0.0",
    "tslint-react-hooks": "2.1.1",
    "typescript": "3.5.2",
    "webpack": "4.35.2",
    "webpack-merge": "4.2.1"
  },
  "engines": {
    "node": ">=10",
    "yarn": ">=1.16"
  }
}
